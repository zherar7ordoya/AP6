            customers =3D new List&lt;ICustomer&gt;();
            customers.Add(new Visitor());
            customers.Add(new Customer());
        }
        public static ICustomer Create(int CustomerType)
        {
            if (customers =3D=3D null)
            {
                LoadCustomers();
            }
