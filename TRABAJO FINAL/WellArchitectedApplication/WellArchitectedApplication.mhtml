using Microsoft.Data.SqlClient;

namespace ADONetLibrary
{
    public abstract class TemplateADO&lt;InjectType&gt;:AbstractDal&lt;Inje=
ctType&gt;
    {
        //Declaring connection and command objects
        protected SqlConnection objConn =3D null;
        protected SqlCommand objCommand =3D null;

        IUow uowObj =3D null;
        public override void SetUnitWOrk(IUow uow)
        {
            // In this one open the connection with transaction attacthed w=
ith it.   =20
            uowObj =3D uow;           =20
            objConn =3D ((AdoNetUow)uow).Connection;
            objCommand =3D new SqlCommand();
            objCommand.Connection =3D objConn;
            objCommand.Transaction =3D ((AdoNetUow)uow).Transaction;
            Save();
        }

        //Pass Inject the connection string as required by the base class
        public TemplateADO(string _connStr):base(_connStr)
        {

        }
        private void OpenConn()
        {

            if (objConn=3D=3Dnull) // IF the transaction connection is not =
open then open a new connection without Transactoin
            {
                objConn =3D new SqlConnection(ConnectionString);
                objConn.Open();
                objCommand =3D new SqlCommand();
                objCommand.Connection =3D objConn;
            }          =20
        }
        private void CloseConn()
        {
            if (uowObj=3D=3Dnull)
            {
                objConn.Close();
            }          =20
        }
       =20
        //This will be overridden by the child class as per their sql execu=
tion queries.
        // they can attach a search, insert or update queries.=20
        protected abstract void ExecuteCommand(InjectType obj);
        protected abstract List&lt;InjectType&gt; ExecuteCommand();

        public void Execute(InjectType obj) // This does the insert=20
        {
            ///Follow the fixed ADO.net template=20
            OpenConn();
            ExecuteCommand(obj);
            CloseConn();

        }

        public List&lt;InjectType&gt; Execute() // returning the list=20
        {
            ///Follow the fixed ADO.net template=20
            List&lt;InjectType&gt; lst =3D null;
            OpenConn();
            lst =3D  ExecuteCommand();
            CloseConn();
            return lst;
        }

        // override the save function from the ABstractDal
        public override void Save()
        {
            foreach (InjectType obj in lstObj)
            {
                Execute(obj);
            }
        }
        public override List&lt;InjectType&gt; Search()
        {
            return Execute();
        }
    }
}
